@{
    ViewBag.Title = "Makale Yönetimi";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

@using Microsoft.AspNetCore.Http
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor



<head>
    <link href="~/Template/dist/css/style.min.css" rel="stylesheet">

    <link href="~/Template/AngularComponents/treeselect/vue-treeselect.css" rel="stylesheet" />
    <link href="~/Template/assets/node_modules/switchery/dist/switchery.css" rel="stylesheet" />
    <script src="~/Template/AngularComponents/treeselect/vue.js"></script>
    <script src="~/Template/AngularComponents/treeselect/vue-treeselect.js"></script>
    <script src="~/Template/AngularComponents/treeselect/index.js"></script>
    <script src="//angular-ui.github.io/bootstrap/ui-bootstrap-tpls-0.11.0.js"></script>
    <script src="~/ScriptController/assayController.js"></script>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <script src="~/Template/ckeditor/samples/toolbarconfigurator/lib/codemirror/codemirror.js"></script>
    <link href="~/Template/ckeditor/samples/toolbarconfigurator/lib/codemirror/codemirror.css" rel="stylesheet" />
</head>

<body ng-app="MyApp" ng-controller="assayController">
    <aside class="left-sidebar">
        <!-- Sidebar scroll-->
        <div class="scroll-sidebar">
            <!-- Sidebar navigation-->
            <nav class="sidebar-nav">
                <ul id="sidebarnav">
                    @*<li class="user-pro">
                            <a class="has-arrow waves-effect waves-dark" href="javascript:void(0)" aria-expanded="false"><img src="~/Template/assets/images/users/1.jpg" alt="user-img" class="img-circle"><span class="hide-menu">@HttpContextAccessor.HttpContext.Session.GetString("NameSurname")</span></a>
                            <ul aria-expanded="false" class="collapse">
                                <li><a href="/Admin/User/Profile"><i class="ti-user"></i> Profil</a></li>
                                <li><a href="javascript:void(0)"><i class="ti-wallet"></i> My Balance</a></li>
                                <li><a href="javascript:void(0)"><i class="ti-email"></i> Inbox</a></li>
                                <li><a href="javascript:void(0)"><i class="ti-settings"></i> Ayarlar</a></li>
                                <li><a href="javascript:void(0)"><i class="fa fa-power-off"></i> Çıkış</a></li>
                            </ul>
                        </li>*@
                    <li ng-repeat="x in menuList">
                        <a class="has-arrow waves-effect waves-dark" href="javascript:void(0)" aria-expanded="false"><i class="{{x.Icon }}"></i><span class="hide-menu" ng-bind="x.Name"> </span></a>
                        <ul aria-expanded="false" class="collapse">
                            <li ng-repeat="y in x.Details">
                                <a href="/{{y.Url}}"><span ng-bind="y.Name"></span> </a>
                            </li>
                        </ul>
                    </li>
                    <li> <a class="waves-effect waves-dark" href="../documentation/documentation.html" aria-expanded="false"><i class="far fa-circle text-danger"></i><span class="hide-menu">Documentation</span></a></li>
                    <li> <a class="waves-effect waves-dark" href="pages-login.html" aria-expanded="false"><i class="far fa-circle text-success"></i><span class="hide-menu">Log Out</span></a></li>
                    <li> <a class="waves-effect waves-dark" href="pages-faq.html" aria-expanded="false"><i class="far fa-circle text-info"></i><span class="hide-menu">FAQs</span></a></li>
                </ul>
            </nav>
            <!-- End Sidebar navigation -->
        </div>
        <!-- End Sidebar scroll-->
    </aside>


    <div class="page-wrapper">
        <!-- ============================================================== -->
        <!-- Container fluid  -->
        <!-- ============================================================== -->
        <div class="container-fluid">
            <!-- ============================================================== -->
            <!-- Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <div class="row page-titles">
                <div class="col-md-5 align-self-center">
                    <h4 class="text-themecolor">Makale Yönetimi</h4>
                </div>
                <div class="col-md-7 align-self-center text-right">
                    <div class="d-flex justify-content-end align-items-center">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="javascript:void(0)">Makale Yönetimi</a></li>
                            <li class="breadcrumb-item active">Makale  Yönetimi</li>
                        </ol>
                    </div>
                </div>
            </div>
            <!-- ============================================================== -->
            <!-- End Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- Start Page Content -->
            <!-- ============================================================== -->
            <!-- Row -->
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-body">
                            <ul class="nav nav-tabs" role="tablist">
                                <li class="nav-item" ng-click="onClickDashboard()"> <a id="dashboardTab" class="nav-link active" data-toggle="tab" href="#home" role="tab"><span class="hidden-sm-up"><i class="ti-home"></i></span> <span class="hidden-xs-down">DashBoard</span></a> </li>
                                <li class="nav-item" ng-click="onClickContent()"> <a id="contentTab" class="nav-link" data-toggle="tab" href="#AssayCreate" role="tab"><span class="hidden-sm-up"><i class="ti-user"></i></span> <span class="hidden-xs-down">Haber {{assayHeadrName}}</span></a> </li>
                            </ul>
                            <div class="tab-content tabcontent-border">

                                <div class="tab-pane active" id="home" role="tabpanel">
                                    <br />
                                    <div class="row" ng-model="rowdata">
                                        <!-- Column -->
                                        <div class="col-md-6 col-lg-3 col-xlg-3">
                                            <div class="card">
                                                <div class="box bg-info text-center">
                                                    <h1 class="font-light text-white"> <span ng-hide="DashboardData.length>0"><i class="fa fa-spinner fa-spin fa-spin"></i></span><span ng-show="DashboardData.length>0" ng-bind="DashboardData[0].count+DashboardData[1].count+DashboardData[2].count+DashboardData[3].count"></span></h1>
                                                    <h6 class="text-white"><span ng-bind="DashboardData[0].description"></span></h6>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- Column -->
                                        <!-- Column -->
                                        <div class="col-lg-3 col-md-6">
                                            <div class="card">
                                                <div class="card-body">
                                                    <div class="row p-t-10 p-b-10">
                                                        <!-- Column -->
                                                        <div class="col p-r-0">
                                                            <h1 class="font-light" ng-bind="contentDashboard[1].quantity"></h1>
                                                            <h6 class="text-muted">Yayınlanmış</h6>
                                                        </div>
                                                        <!-- Column -->
                                                        <div class="col text-right align-self-center">
                                                            <div data-label="{{contentDashboard[1].percent}}%" class="css-bar m-b-0 css-bar-danger css-bar-20"><i class="mdi mdi-briefcase-check"></i></div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- Column -->
                                        <!-- Column -->
                                        <div class="col-lg-3 col-md-6">
                                            <div class="card">
                                                <div class="card-body">
                                                    <div class="row p-t-10 p-b-10">
                                                        <!-- Column -->
                                                        <div class="col p-r-0">
                                                            <h1 class="font-light" ng-bind="contentDashboard[2].quantity"></h1>
                                                            <h6 class="text-muted">Taslak</h6>
                                                        </div>
                                                        <!-- Column -->
                                                        <div class="col text-right align-self-center">
                                                            <div data-label="{{contentDashboard[2].percent}}%" class="css-bar m-b-0 css-bar-warning css-bar-40"><i class="mdi mdi-star-circle"></i></div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- Column -->
                                        <!-- Column -->
                                        <div class="col-lg-3 col-md-6">
                                            <div class="card">
                                                <div class="card-body">
                                                    <div class="row p-t-10 p-b-10">
                                                        <!-- Column -->
                                                        <div class="col p-r-0">
                                                            <h1 class="font-light" ng-bind="contentDashboard[3].quantity"></h1>
                                                            <h6 class="text-muted">Yayınlanmamış</h6>
                                                        </div>
                                                        <!-- Column -->
                                                        <div class="col text-right align-self-center">
                                                            <div data-label="{{contentDashboard[3].percent}}%" class="css-bar m-b-0 css-bar-info css-bar-60"><i class="mdi mdi-receipt"></i></div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- Column -->
                                    </div>
                                    <br />
                                    <br />
                                    <div class="center" ng-show="showTable">
                                        <span><i class="fa fa-spinner fa-spin"></i></span>
                                    </div>
                                    <div class="row" ng-show="!showTable">
                                        <div class="col-lg-12">

                                            <div class="row">
                                                <div class="col-md-2">
                                                    <div class="form-group">
                                                        <label>Başlangıç Tarihi</label>
                                                        <input id="searchStartDate" type="text" class="form-control" placeholder="dd.mm.yyyy" />
                                                    </div>
                                                </div>
                                                <div class="col-md-2">
                                                    <div class="form-group">
                                                        <label>Bitiş Tarihi</label>
                                                        <input id="searchEndDate" type="text" class="form-control" placeholder="dd.mm.yyyy" />
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label>Makale Kategori</label>
                                                        <input type="hidden" id="searhCategoryIds" />
                                                        <treeselect v-props-options="options" v-props-field="'searhCategoryIds'" v-props-multiple="true" v-props-value="value" watch-depth="value"></treeselect>
                                                    </div>
                                                </div>
                                                <div class="col-md-2">
                                                    <div class="form-group">
                                                        <label><span></span></label>
                                                        <button ng-click="shoShortListAssay()" type="button" class="btn btn-primary">Ara</button>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="card">

                                                <div class="card-body">
                                                    <h4 class="card-title">Makaleler Listesi</h4>
                                                    <h6 class="card-subtitle">Makale Sayısı : <code><span ng-bind="assayList.length"></span></code></h6>
                                                    <h4><input type="text" class="form-control" name="site-search" id="getData" ng-model="search" ng-keypress="getData($event)" placeholder="Ara..."></h4>
                                                    <table>
                                                        <tr>
                                                            <td>
                                                                <button class="btn btn-block btn-primary" ng-disabled="currentPage == 0" ng-click="currentPage=currentPage-1">Önceki </button>
                                                            </td>
                                                            <td>
                                                                &nbsp;{{currentPage+1}}/{{numberOfPages()}}&nbsp;
                                                            </td>
                                                            <td>
                                                                <button class="btn btn-block btn-primary" ng-disabled="currentPage >= getData().length/pageSize - 1" ng-click="currentPage=currentPage+1"> Sonraki</button>
                                                            </td>
                                                        </tr>
                                                    </table>
                                                    <div class="table-responsive">

                                                        <table class="table">
                                                            <thead>
                                                                <tr>
                                                                    <th>Başlık</th>
                                                                    <th>Yazar</th>
                                                                    <th>Yayınlanma Durumu</th>
                                                                    <th>Platform</th>
                                                                    <th>Okunma</th>
                                                                    <th>Oluşturma Tarihi</th>
                                                                    <th>Yayınlanma Tarihi</th>
                                                                    <th>Detay</th>
                                                                    <th>Yorumlar</th>
                                                                    @if (ViewBag.showColumn == 1 || ViewBag.showColumn == 2 || ViewBag.showColumn == 4)
                                                                    {
                                                                        <th>İşlem</th>
                                                                    }

                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="x in assayList| filter:search | startFrom:currentPage*pageSize | limitTo:pageSize" )">
                                                                    <td ng-show="false" ng-bind="x.id"></td>
                                                                    <td ng-bind="x.header"></td>
                                                                    <td ng-bind="x.writer"></td>
                                                                    <td ng-show="false" ng-bind="x.platformType"></td>
                                                                    <td ng-bind="x.publishStateTypeDes"></td>
                                                                    <td ng-bind="x.platformTypeDes"></td>
                                                                    <td ng-bind="x.readCount"></td>
                                                                    <td ng-bind="x.createdDate | date:'MM/dd/yyyy HH:mm:ss'"></td>
                                                                    <td ng-bind="x.publishDate | date:'MM/dd/yyyy HH:mm:ss'"></td>
                                                                    <td>
                                                                        <button ng-click="showContentDetail(x)" type="button" class="btn btn-rounded btn-success">Aç</button>
                                                                    </td>
                                                                    <td>
                                                                        <button ng-click="showComment(x.id)" type="button" class="btn btn-rounded btn-success" @*data-toggle="modal" data-target="#commentModal"*@>Aç</button>
                                                                    </td>
                                                                    @if (ViewBag.showColumn == 1 || ViewBag.showColumn == 2 || ViewBag.showColumn == 4)
                                                                    {
                                                                        <td>
                                                                            <button ng-click="onClikckProcess(x)" type="button" class="btn btn-rounded btn-success" data-toggle="modal" data-target="#contentProcessModal">İşlem</button>
                                                                        </td>
                                                                    }

                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>

                                <div class="modal fade bs-example-modal-lg" id="commentModal" tabindex="-1" role="dialog" aria-labelledby="commentModal">
                                    <div class="modal-dialog" role="document">
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <h4 class="modal-title" id="exampleModalLabel1">Yorumlar</h4>
                                                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                                            </div>
                                            <div class="modal-body">
                                                <div class="table-responsive">

                                                    <table class="table">
                                                        <thead>
                                                            <tr>
                                                                <th>Kullanıcı Adı</th>
                                                                <th>Yorum</th>
                                                                <th>Tarih</th>
                                                                <th>İşlem</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="x in commentList )">
                                                                <td ng-show="false" ng-bind="x.id"></td>
                                                                <td ng-bind="x.userName"></td>
                                                                <td ng-bind="x.comment"></td>
                                                                <td ng-bind="x.postDate | date:'MM/dd/yyyy HH:mm:ss'"></td>
                                                                <td>
                                                                    <button ng-click="ApproveComment(x.id)" type="button" class="btn btn-rounded btn-{{x.buttonClass}}">{{x.buttonName}}</button>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>


                                <div class="modal fade" id="contentProcessModal" tabindex="-1" role="dialog" aria-labelledby="contentProcessModal">
                                    <div class="modal-dialog modal-lg" role="document">
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <h4 class="modal-title" id="exampleModalLabel1">Haber Yönetim -{{contentProcess.header}} </h4>
                                                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                                            </div>
                                            <div class="modal-body">
                                                <div class="card">
                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="col-md-3">
                                                                <div class="form-group">
                                                                    <label class="control-label">Haber Yayın Durumu </label>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-5">
                                                                <div class="form-group">
                                                                    <select class="form-control custom-select" ng-options="v.Description for v in confirmType track by v.Id" ng-model="contentProcess.publishState"></select>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-2">
                                                                <div class="form-group">
                                                                    <button type="button" class="btn btn-primary" ng-click="postPublishState()"> <span>Güncelle <span ng-show="contentModel.contentPublishStateShowLoading"><i class="fa fa-spinner fa-spin"></i></span></span> </button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="card">
                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="col-md-5">
                                                                <div class="form-group">
                                                                    <label class="control-label">Okunma Sayısı Çarpan Değeri </label>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-2">
                                                                <div class="form-group">
                                                                    <select class="form-control custom-select" ng-options="v.Description for v in contentModel.multipleList track by v.Id" ng-model="contentModel.selectedId"></select>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-1">
                                                                <span ng-show="contentModel.showContentCountLoading"><i class="fa fa-spinner fa-spin"></i></span>
                                                            </div>
                                                            <div class="col-md-2">
                                                                <div class="form-group">
                                                                    <button type="button" class="btn btn-primary" ng-click="postContent()"> <span>Güncelle <span ng-show="contentModel.showContentCountPostLoading"><i class="fa fa-spinner fa-spin"></i></span></span> </button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                            <div class="modal-footer">
                                                <button type="button" class="btn btn-default" data-dismiss="modal">Kapat</button>
                                                @*<button type="button" class="btn btn-primary" ng-click="postPublishState()">İşlem Yap <span ng-show="showSaveLoading"><i class="fa fa-spinner fa-spin"></i></span></button>*@
                                            </div>
                                        </div>
                                    </div>
                                </div>




                                <div ng-disabled="showSaveLoading" class="tab-pane  p-20" id="AssayCreate" role="tabpanel">
                                    <div class="row">
                                        <div class="col-md-12">

                                            <div class="col-md-10 p-0">

                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label>Makale Başlığı</label>
                                                            <input type="text" class="form-control" ng-model="assayCreate.header">
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label>Seo Link</label>
                                                            <input type="text" id="seoUrl" ng-disabled="true" class="form-control" placeholder="Seo Link" value="{{RegexSeo(assayCreate.header)}}" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="form-group">
                                                            <label>Makale Başlık Resim</label>
                                                            <input type="text" class="form-control" id="roxyField" onclick="openCustomRoxy2('#roxyCustomPanel2')" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-2">
                                                        <div class="form-group">
                                                            <label>Yayınlanma Tarihi</label>
                                                            <input id="publishDateAssay" type="text" class="form-control" placeholder="dd.mm.yyyy" />
                                                        </div>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <div class="form-group">
                                                            <label>Yayınlanma Saati</label>
                                                            <input id="publishHour" type="text" class="form-control" placeholder="HH:mm" />
                                                        </div>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <div class="form-group">
                                                            <label>Yayınlansın mı?</label>
                                                            <br />
                                                            <input id="togglwPublish" type="checkbox" ng-model="assayCreate.isHeadLine" class="js-switch" data-color="#00c292" />
                                                        </div>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <div class="form-group">
                                                            <label>Manşet mi?</label>
                                                            <br />
                                                            <input id="toggleManset" type="checkbox" ng-checked="{{assayCreate.isManset}}" class="js-switch" data-color="#00c292" />
                                                        </div>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <div class="form-group">
                                                            <label>Ana Menü?</label>
                                                            <br />
                                                            <input id="toggleMainMenu" type="checkbox" ng-checked="{{assayCreate.isMainMenu}}" class="js-switch" data-color="#00c292" />
                                                        </div>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <div class="form-group">
                                                            <label>Ana Menüde Sabitlensin mi?</label>
                                                            <br />
                                                            <input id="toggleisConstantMainMenu" type="checkbox" ng-checked="{{assayCreate.isConstantMainMenu}}" class="js-switch" data-color="#00c292" />
                                                        </div>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <div class="form-group">
                                                            <label>Onaya Gönderilsin mi?</label>
                                                            <br />
                                                            <input id="toggleisSendConfirm" type="checkbox" ng-checked="{{assayCreate.isSendConfirm}}" class="js-switch" data-color="#00c292" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-2">
                                                        <div class="form-group">
                                                            <label class="control-label">Etkinlikler</label>
                                                            <select class="form-control custom-select" ng-options="v.Description for v in events track by v.Id" ng-model="assayCreate.eventId"></select>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <div class="form-group">
                                                            <label class="control-label">Stajlar</label>
                                                            <select class="form-control custom-select" ng-options="v.Description for v in internList track by v.Id" ng-model="assayCreate.internId"></select>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <div class="form-group">
                                                            <label class="control-label">Görünürlük</label>
                                                            <select class="form-control custom-select" ng-options="v.Description for v in visibleList track by v.Id" ng-model="assayCreate.visibleId"></select>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <div class="form-group">
                                                            <label class="control-label">Platform Türü</label>
                                                            <select class="form-control custom-select" ng-options="v.Description for v in platformTypeList track by v.Id" ng-model="assayCreate.platformTypeId"></select>


                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-2 text-right">
                                                <img class="img-responsive newsImage" id="img_roxyField" />
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <label>Makale Kategori</label>
                                                <input type="hidden" id="categoryIds" />
                                                <treeselect v-props-options="options" v-props-field="'categoryIds'" v-props-multiple="true" v-props-value="categoryValue" watch-depth="value"></treeselect>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <label>Makale Etiket</label>
                                                <input type="hidden" value="" id="multipleTags" />
                                                <treeselect v-props-options="tagOptions" v-props-field="'multipleTags'" v-props-is-insertable="true" v-props-multiple="true" v-props-value="value" watch-depth="value"></treeselect>
                                            </div>
